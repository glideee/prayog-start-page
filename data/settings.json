					}
				]
			},
			{
				"title": "Files & Study",
				"color": "yellow",
				"align": "left",
				"links": [
					{
						"name": "Schedule",
						"url": "https://schedulee.pages.dev/",
						"icon": "mdi:clock-outline"
					},
					{
						"name": "Teachers",
